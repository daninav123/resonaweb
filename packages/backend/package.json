{
  "name": "backend",
  "version": "1.0.0",
  "description": "ReSona Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "dev:quick": "cross-env TS_NODE_TRANSPILE_ONLY=true ts-node src/index.ts",
    "build": "node build-railway.js",
    "start": "node dist/index.js",
    "postinstall": "prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testMatch='**/__tests__/**/*.test.ts'",
    "test:integration": "jest --testMatch='**/tests/integration/**/*.test.ts'",
    "test:e2e": "node test-api.js",
    "test:e2e:basic": "node test-api-complete.js",
    "test:e2e:extended": "node test-api-extended.js",
    "test:e2e:all": "node test-all.js",
    "test:e2e:jest": "jest --testMatch='**/tests/e2e/**/*.test.ts'",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:seed": "ts-node src/scripts/seed.ts",
    "db:seed:force": "cross-env FORCE_SEED=true ts-node src/scripts/seed.ts",
    "db:seed:blog": "ts-node src/scripts/seed-blog-data.ts",
    "db:seed:packs": "ts-node src/scripts/seed-packs.ts",
    "db:seed:product-packs": "ts-node src/scripts/seed-product-packs.ts",
    "db:create:packs-category": "ts-node src/scripts/create-packs-category.ts",
    "db:reset": "ts-node src/scripts/reset-database.ts",
    "db:test-data": "ts-node src/scripts/create-test-data.ts",
    "db:update-prices": "ts-node scripts/update-product-prices.ts",
    "db:reorganize-skus": "ts-node scripts/reorganize-skus.ts",
    "db:studio": "prisma studio",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@prisma/client": "^5.7.0",
    "@sendgrid/mail": "^8.1.6",
    "@types/pdfkit": "^0.17.3",
    "archiver": "^6.0.1",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.3",
    "bull": "^4.11.5",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "handlebars": "^4.7.8",
    "he": "^1.2.0",
    "helmet": "^7.1.0",
    "html-entities": "^2.6.0",
    "ical-generator": "^10.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.10",
    "openai": "^6.8.1",
    "pdfkit": "^0.17.2",
    "prom-client": "^15.1.3",
    "redis": "^4.6.11",
    "redsys-easy": "^5.3.0",
    "redsys-pos": "^1.0.2",
    "resend": "^6.4.2",
    "sharp": "^0.34.5",
    "slugify": "^1.6.6",
    "stripe": "^14.25.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.11.0",
    "xmlbuilder2": "^4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.25",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "cross-env": "^10.1.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
